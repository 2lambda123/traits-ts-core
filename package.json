{
    "name":                              "@rse/traits",
    "publishConfig":                     { "access": "public" },
    "description":                       "Traits for TypeScript Classes",
    "keywords":                          [ "traits", "class", "mixin" ],
    "homepage":                          "http://github.com/rse/traits",
    "repository":                        { "url": "git+https://github.com/rse/traits.git", "type": "git" },
    "bugs":                              { "url": "http://github.com/rse/traits/issues" },
    "version":                           "0.9.2",
    "license":                           "MIT",
    "author": {
        "name":                          "Dr. Ralf S. Engelschall",
        "email":                         "rse@engelschall.com",
        "url":                           "http://engelschall.com"
    },
    "types":                             "./dst/traits.d.ts",
    "module":                            "./dst/traits.esm.js",
    "main":                              "./dst/traits.cjs.js",
    "browser":                           "./dst/traits.umd.js",
    "exports": {
        ".": {
            "import":                    { "types": "./dst/traits.d.ts", "default": "./dst/traits.esm.js" },
            "require":                   { "types": "./dst/traits.d.ts", "default": "./dst/traits.cjs.js" }
        }
    },
    "devDependencies": {
        "eslint":                        "9.18.0",
        "@eslint/js":                    "9.18.0",
        "eslint-plugin-n":               "17.15.1",
        "eslint-plugin-promise":         "7.2.1",
        "eslint-plugin-import":          "2.31.0",
        "eslint-plugin-mocha":           "10.5.0",
        "eslint-plugin-chai-expect":     "3.1.0",
        "neostandard":                   "0.12.0",
        "globals":                       "15.14.0",

        "vite":                          "6.0.7",
        "vite-plugin-singlefile":        "2.1.0",
        "vite-plugin-node-polyfills":    "0.22.0",
        "@wroud/vite-plugin-tsc":        "0.11.5",
        "typescript":                    "5.7.3",
        "tsx":                           "4.19.2",
        "jiti":                          "2.4.2",
        "mocha":                         "11.0.1",
        "chai":                          "5.1.2",
        "chai-as-promised":              "8.0.1",
        "sinon-chai":                    "4.0.0",
        "sinon":                         "19.0.2",

        "nps":                           "5.10.0",
        "nodemon":                       "3.1.9",
        "shx":                           "0.3.4",

        "@types/node":                   "22.10.7",
        "@types/chai":                   "5.0.1",
        "@types/mocha":                  "10.0.10",
        "@types/sinon":                  "17.0.3",
        "@types/sinon-chai":             "4.0.0",
        "@types/chai-as-promised":       "8.0.1"
    },
    "dependencies": {
        "eventemitter2":                 "6.4.9"
    },
    "overrides": {
        "vite-plugin-node-polyfills":    { "vite": "^6.0.0" }
    },
    "scripts": {
        "start":                         "nps -c etc/nps.yaml"
    }
}
